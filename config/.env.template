# Environment Configuration Template
# Copy to .env and fill in your values

# Google AI API Key (Required)
# Get from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Project Settings (Required)
GCP_PROJECT_ID=your_gcp_project_id
GCS_BUCKET_NAME=dow30-financial-pipeline

# Optional: Service Account (for production)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Optional: Gemini Settings
# GEMINI_MODEL=gemini-1.5-pro-latest
# GEMINI_TEMPERATURE=0.1
# GEMINI_MAX_TOKENS=8192

# Optional: GCS Settings
# GCS_LOCATION=US

# Optional: Airflow Settings
# AIRFLOW_DAG_ID=dow30_financial_pipeline
# AIRFLOW_SCHEDULE=@daily
# AIRFLOW_CATCHUP=False
# AIRFLOW_MAX_RUNS=1
# AIRFLOW_RETRIES=2
